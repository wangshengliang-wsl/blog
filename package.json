{
  "name": "astro-antfustyle-theme",
  "version": "2.3.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "dev": "astro dev --open",
    "check": "astro check",
    "build": "./scripts/build.sh",
    "preview": "astro preview",
    "format": "prettier . --check --ignore-unknown",
    "format:write": "prettier . --write --ignore-unknown --cache",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "eslint . --fix --cache",
    "toolbar:on": "astro preferences enable devToolbar",
    "toolbar:off": "astro preferences disable devToolbar",
    "prepare": "simple-git-hooks",
    "postbuild": "pagefind --site dist --exclude-selectors \"pre\" --include-characters \"<>\" && del-cli \"dist/pagefind/*ui*\"",
    "deploy": "pnpm build && wrangler pages deploy dist --project-name=blog"
  },
  "devDependencies": {
    "@ascorbic/feed-loader": "^1.0.4",
    "@astrojs/check": "^0.9.6",
    "@astrojs/markdown-remark": "^6.3.9",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "@atproto/api": "^0.17.7",
    "@eslint/js": "^9.39.1",
    "@expressive-code/plugin-collapsible-sections": "^0.41.3",
    "@expressive-code/plugin-line-numbers": "^0.41.3",
    "@iconify/json": "^2.2.412",
    "@types/node": "^24.10.1",
    "@types/nprogress": "^0.2.3",
    "@types/p5": "^1.7.7",
    "@typescript-eslint/parser": "^8.48.0",
    "@unocss/reset": "66.5.3",
    "astro": "^5.16.3",
    "astro-expressive-code": "^0.41.3",
    "astro-loader-bluesky-posts": "^1.2.3",
    "astro-loader-github-prs": "^1.3.1",
    "astro-loader-github-releases": "^2.1.1",
    "astro-robots-txt": "^1.0.0",
    "chalk": "^5.6.2",
    "del-cli": "^6.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.5.0",
    "html-entities": "^2.6.0",
    "lint-staged": "^15.5.2",
    "mdast-util-to-string": "^4.0.0",
    "nprogress": "^0.2.0",
    "p5": "^1.11.11",
    "pagefind": "^1.4.0",
    "prettier": "^3.7.3",
    "prettier-plugin-astro": "^0.14.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-callouts": "^2.1.2",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-wrap-all": "^1.1.0",
    "remark-directive": "^4.0.0",
    "remark-directive-sugar": "^1.1.2",
    "remark-imgattr": "^1.0.5",
    "remark-math": "^6.0.0",
    "satori": "^0.18.3",
    "satori-html": "^0.3.2",
    "sharp": "^0.34.5",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "unist-util-visit": "^5.0.0",
    "unocss": "66.5.3",
    "viewerjs": "^1.11.7"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{js,mjs,cjs,ts,tsx,astro,json,md,mdx}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "simple-git-hooks"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
